
@{
    ViewBag.Title = "Users";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayoutPage1.cshtml";
}

<h2>Users</h2>

<table class="table table-bordered">
    <tr>
        <th>UserName</th>
        <th>Email</th>
        <th>DateOfBirth</th>
        <th>Mobile</th>
        <th>City</th>
        <th>Country</th>
    </tr>

    @foreach (var item in Model)
    { 
        <tr>
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td>@item.Birthday</td>
            <td>@item.PhoneNumber</td>
            <td>@item.City</td>
            <td>@item.Country</td>
        </tr>
    }
</table>

