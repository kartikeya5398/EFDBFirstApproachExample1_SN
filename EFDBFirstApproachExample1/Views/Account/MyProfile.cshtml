@model EFDBFirstApproachExample1.Identity.ApplicationUser 
@{
    ViewBag.Title = "MyProfile";
    
}

<h2>MyProfile</h2>

<table class="table-bordered">

    <tr>
        <th>UserName</th>
        <td>@Model.UserName</td>
    </tr>

    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>

    <tr>
        <th>Date Of Birth</th>
        <td>@Model.Birthday</td>
    </tr>

    <tr>
        <th>Mobile</th>
        <td>@Model.PhoneNumber</td>
    </tr>

    <tr>
        <th>City</th>
        <td>@Model.City</td>
    </tr>

    <tr>
        <th>Country</th>
        <td>@Model.Country</td>
    </tr>

    
</table>
<a href="/Home/index">Back</a>

